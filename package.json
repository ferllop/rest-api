{
  "name": "almighty-simple-rest-api",
  "version": "1.0.3",
  "description": "A simple rest api",
  "main": "./build/index.js",
  "exports": {
    ".": "./build.index.js"
  },
  "type": "module",
  "scripts": {
    "compile": "tsc -p tsconfig.build.json",
    "build": "npm run compile",
