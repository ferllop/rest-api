  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node16": "1.0.2",
    "@types/node": "17.0.18",
    "nodemon": "^2.0.9",
    "tsm": "2.2.1",
    "typescript": "4.5.5",
    "uvu": "^0.5.1"
  },
  "dependencies": {
    "preconditions": "ferllop/preconditions#semver:1.x.x"
  }
}
